{
  "extends": "@sissel/tsconfig",
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/index.ts"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "incremental": false,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "noEmit": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "target": "ES6",
    "module": "CommonJS"
  }
}